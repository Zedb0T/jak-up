    (set! (-> v1-8 local-pos z) -12083.2)
    (set! (-> v1-8 local-pos w) 1.0)
    )
  (let ((v1-10 (-> this control-point-array data 2)))
    (set! (-> v1-10 local-pos x) -9216.0)
